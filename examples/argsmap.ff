Def("Print", [x: "Hello World!"], {
    Write(stdout, x)
})
Def("Sum", [a: 2, b], {
    Set(y, ": ")
    Write(stdout, Cat("a", y, a))
    Write(stdout, Cat("b", y, b))
})
Def("Main", [], {
    Print("OK.")
    Print()
    Print(x: "oh what")
    Sum(7, 12)
    Sum(3)
    Sum(8, 1, c: 3)
    Sum(a: 3, 5)
    Sum(6, b: 4)
})
Main()