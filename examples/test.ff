set(pow, [x, n]{
    if((n=2), {
        set(x, mul(x, x))
    }, {
        set(x, mul(x, pow(x, add(n, -1))))
    })
})

set(quitloop, [] {
    set(x, "")
    for(!(x="QUIT"), {
        write(stdout, "Type QUIT to exit")
        set(x, read(stdin))
    })
})

set(print, [x] {
    write(stdout, x)
})

set(main, [] {
    quitloop()
    print([1, 2.2, 3, 4.36])
})

main()
