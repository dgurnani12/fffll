Def(pow, [x, n], {
    If((n=2), {
	  Set(x, Mul(x, x))
      }, {
	  Set(x, Mul(x, Pow(x, Add(n, -1))))
      })
})

Def(quitLoop, [], {
    Set(x, "")
    For(!(x="QUIT"), {
        Write(stdout, "Type QUIT to exit")
        Set(x, Read(stdin))
    })
})

Def(print, [x], {
    Write(stdout, x)
})

Def(main, [], {
    Quitloop()
    Print([1, 2.2, 3, 4.36])
})

Main()
